{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_GEMINI_API_KEY": "@vite_gemini_api_key",
    "VITE_GOOGLE_API_KEY": "@vite_google_api_key",
    "VITE_GOOGLE_CSE_ID": "@vite_google_cse_id"
  }
} 